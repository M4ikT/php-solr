#!/usr/bin/env php
<?php
spl_autoload_register(
  function ($class)
  {
      static $classes = NULL;

      if ($classes === NULL) {
          $classes = array(
            ___CLASSLIST___
          );
      }

      $class = strtolower($class);

      if (isset($classes[$class])) {
          require 'phar://phpdox.phar/' . $classes[$class];
      }
  }
);

Phar::mapPhar('php-solr.phar');
define('PHP-SOLR_PHAR', 'php-solr.phar');

exit(0);

__HALT_COMPILER();
